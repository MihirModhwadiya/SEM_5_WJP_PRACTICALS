<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.pra3.StudentEntity" table="student">
        <id name="studentId">
            <column name="student_id" sql-type="int(11)"/>
        </id>
        <property name="studentName">
            <column name="student_name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="branch">
            <column name="branch" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>